public class CompteCourant extends Compte {

    
    public CompteCourant (int tauxInteret, int sold) {
        super(tauxInteret, sold);
    }

   
    public int retreat (int debit) {
        if(this.sold > debit){
            this.sold -= debit;
            return debit;
        }
        return 0;
    }

    // <editor-fold defaultstate="collapsed" desc=" UML Marker "> 
    // #[regen=yes,id=DCE.883A1865-03D5-42C0-B040-BCC7AC7F0414]
    // </editor-fold> 
    public void getSold () {
    }

}


public abstract class Compte {

    private final double TAUX_INTERET;
    protected double sold;

    public Compte (double tauxInteret, double sold) {
        this.TAUX_INTERET = tauxInteret;
        this.sold = sold;
    }

    public void setSold (int val) {
        this.sold = val;
    }
    
    public void getSold(){
        System.out.println("Le solde de votre Compte de "+this.sold);
    }

    // <editor-fold defaultstate="collapsed" desc=" UML Marker "> 
    // #[regen=yes,regenBody=yes,id=DCE.851CF9FB-31C0-C950-4A68-054603987038]
    // </editor-fold> 
    public void setSold (double val) {
        this.sold = val;
    }

}


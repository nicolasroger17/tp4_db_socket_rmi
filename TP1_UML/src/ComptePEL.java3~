public class ComptePEL extends Compte {

    private int MinCap;

    private final int MIN_CAP;

    public ComptePEL (int tauxInteret, int sold, int minCap) {
           super(tauxInteret, sold);
           this.MIN_CAP = minCap;
    }
    
    @Override
    public void getSold () {
        System.out.println("Le solde de votre PEL est de "+this.sold);
    }

}
public abstract class Compte {

    private final int TAUX_INTERET;
    protected int sold;

    public Compte (int tauxInteret, int sold) {
        this.TAUX_INTERET = tauxInteret;
        this.sold = sold;
    }

    // <editor-fold defaultstate="collapsed" desc=" UML Marker "> 
    // #[regen=yes,regenBody=yes,id=DCE.4AF0E1A5-D695-F68D-ECBA-7FC7A53D92C4]
    // </editor-fold> 
    public int getSold () {
        return sold;
    }

    // <editor-fold defaultstate="collapsed" desc=" UML Marker "> 
    // #[regen=yes,regenBody=yes,id=DCE.851CF9FB-31C0-C950-4A68-054603987038]
    // </editor-fold> 
    public void addCredit (int val) {
        this.sold = val;
    }

}


public class CompteLDD extends Compte {

    private int MaxCap;

    private final int MAX_CAP;

    public CompteLDD (double tauxInteret, double sold, int maxCap) {
        super(tauxInteret, sold);
        this.MAX_CAP = maxCap;
    }
    
    @Override
    public void getSold () {
        System.out.println("Le solde de votre LDD est de "+this.sold);
    }


}

